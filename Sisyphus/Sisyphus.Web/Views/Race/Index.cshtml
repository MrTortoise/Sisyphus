@model System.Collections.Generic.List<Sisyphus.Core.Model.Race>

@{
    ViewBag.Title = "Races";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Races</h2>

@Html.ActionLink("Create Race","Create")

<div class="form-group">
    @foreach (var race in Model)
    {
        @Html.Partial("_race",race)
    }
    
</div>